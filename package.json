{
  "name": "coc-cairo",
  "version": "0.0.1",
  "description": "A coc.nvim wrapper for the Cairo language server",
  "author": "Kevin Halliday <halliday.kevin7@gmail.com>",
  "repository": "https://github.com/kevinhalliday/coc-cairo",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.80"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "node esbuild.js --watch",
    "build": "node esbuild.js",
    "prepare": "node esbuild.js"
  },
  "dependencies": {
    "cairo-ls": "^0.0.7"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "coc.nvim": "^0.0.80",
    "esbuild": "^0.14.24",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-cairo configuration",
      "properties": {
        "cairo.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-cairo extension"
        }
      }
    }
  }
}
